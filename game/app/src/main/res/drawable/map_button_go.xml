<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:state_enabled="false"
        android:drawable="@drawable/map_button_go_disabled"
    />
    <item
        android:state_pressed="true"
        android:state_enabled="true"
        android:drawable="@drawable/map_button_go_pressed"
    />
    <item
        android:state_focused="true"
        android:state_enabled="true"
        android:drawable="@drawable/map_button_go_focused"
    />
    <item
        android:state_enabled="true"
        android:drawable="@drawable/map_button_go_enabled"
    />
</selector>
