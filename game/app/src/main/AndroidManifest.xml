<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.replica.game">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        >

        <activity
            android:name="com.replica.engine.StartupActivity"
            android:label="@string/app_name"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.replica.engine.EndingActivity"
            android:label="@string/app_name"
        />
        <activity
            android:name="SetPreferencesActivityImpl"
        />
        <activity
            android:name="com.replica.engine.LevelSelectActivityImpl"
            android:label="@string/app_name"
        />
        <activity
            android:name="MainMenuActivityImplSpecific"
            android:label="@string/app_name"
            android:launchMode="singleTask"
        />
        <activity
            android:name="MainImpl"
            android:label="@string/app_name"
            android:launchMode="singleTask"
        />
        <activity
            android:name="com.replica.engine.DifficultyMenuActivityImpl"
        />
        <activity
            android:name="com.replica.engine.AnimationPlayerActivityImpl"
        />
        <activity
            android:name="ResultsGameOverActivityImpl"
        />
<!-- //////// lev res - m -->
        <activity
            android:name="ResultsLevelActivityImpl"
        />
<!-- //////// lev res - e -->

    </application>
</manifest>
