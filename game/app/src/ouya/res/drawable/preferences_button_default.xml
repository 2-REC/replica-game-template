<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
  <item
    android:state_enabled="false"
    android:drawable="@drawable/preferences_button_default_disabled"
  />
  <item
    android:state_pressed="true"
    android:state_enabled="true"
    android:drawable="@drawable/preferences_button_default_pressed"
  />
  <item
    android:state_focused="true"
    android:state_enabled="true"
    android:drawable="@drawable/preferences_button_default_focused"
  />
  <item
    android:state_enabled="true"
    android:drawable="@drawable/preferences_button_default_enabled"
  />
</selector>
